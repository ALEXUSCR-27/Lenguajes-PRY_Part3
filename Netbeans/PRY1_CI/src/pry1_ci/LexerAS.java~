						 return symbol(sym.L_CHAR, '\b');
          }
        case 97: break;
        case 41: 
          { return symbol(sym.LESS_THAN_OR_EQ);
          }
        case 98: break;
        case 2: 
          { /*ignore*/
          }
        case 99: break;
        case 6: 
          { return symbol(sym.COMENTARIO);
          }
        case 100: break;
        case 36: 
          { return symbol(sym.DO);
          }
        case 101: break;
        case 63: 
          { return symbol(sym.MAIN);
          }
        case 102: break;
        case 5: 
          { return symbol(sym.TIMES);
          }
        case 103: break;
        case 62: 
          { return symbol(sym.ELSE);
          }
        case 104: break;
        case 46: 
          { string.append('\"');
          }
        case 105: break;
        case 13: 
          { return symbol(sym.PARENTESISCIERRA);
          }
        case 106: break;
        case 48: 
          { return symbol(sym.L_FLOTANTE, Float.valueOf(yytext()));
          }
        case 107: break;
        case 9: 
          { return symbol(sym.FIN_EXPRESION);
          }
        case 108: break;
        case 66: 
          { return symbol(sym.WHILE);
          }
        case 109: break;
